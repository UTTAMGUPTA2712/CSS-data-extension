{
  "name": "CSS data",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "description",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["script.js"],
      "css": ["style.css"]
    }
  ],
  "permissions": ["activeTab", "clipboardWrite"],
  "browser_action": {
    "default_popup": "main.html"
  }
}
